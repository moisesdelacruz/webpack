{
  "name": "curso-de-webpack",
  "version": "1.0.0",
  "description": "curso de webpack in platzi",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack index.js --mode none",
    "build:local": "webpack --mode none",
    "build:external": "webpack --config ./external/webpack.config.js --mode none",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js --mode none",
    "build:extract:css": "webpack --config ./plugin-extract-text/webpack.config.js --mode none",
    "build:multi": "webpack --config ./multiples-entry-points/webpack.config.js --mode none",
    "build:dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js --mode none",
    "build:babel": "webpack --config ./babel-loader/webpack.config.js --mode none",
    "build:images": "webpack --config ./url-loader-images/webpack.config.js --mode none",
    "build:fonts": "webpack --config ./url-loader-fonts/webpack.config.js --mode none",
    "build:video": "webpack --config ./file-loader-video/webpack.config.js --mode none",
    "build:react": "webpack --config ./react/webpack.config.js --mode none",
    "build:sass": "webpack --config ./sass-loader/webpack.config.js --mode none",
    "build:stylus": "webpack --config ./stylus-loader/webpack.config.js --mode none",
    "build:less": "webpack --config ./less-loader/webpack.config.js --mode none"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moisesdelacruz/webpack.git"
  },
  "keywords": [
    "webpack"
  ],
  "author": "moisesdelacruz.dev@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/moisesdelacruz/webpack/issues"
  },
  "homepage": "https://github.com/moisesdelacruz/webpack#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "7",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^1.0.1",
    "file-loader": "^2.0.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.4",
    "nib": "^1.1.2",
    "node-sass": "^4.10.0",
    "rupture": "^0.7.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "react": "^16.6.1",
    "react-dom": "^16.6.1"
  }
